package com.itwill.giga_box.rating;

//평점
public class Rating {
	private int rating_no; //평점 번호
	private double rating_score; //평점
	private String m_id; //회원 아이디
	private int movie_no; //영화번호
	
	public Rating() {
	}
	

	public Rating(int rating_no, double rating_score, String m_id, int movie_no) {
		super();
		this.rating_no = rating_no;
		this.rating_score = rating_score;
		this.m_id = m_id;
		this.movie_no = movie_no;
	}









	public int getRating_no() {
		return rating_no;
	}

	public void setRating_no(int rating_no) {
		this.rating_no = rating_no;
	}

	public String getM_id() {
		return m_id;
	}

	public void setM_id(String m_id) {
		this.m_id = m_id;
	}

	public int getMovie_no() {
		return movie_no;
	}

	public void setMovie_no(int movie_no) {
		this.movie_no = movie_no;
	}

	public double getRating_score() {
		return rating_score;
	}

	public void setRating_score(double rating_score) {
		this.rating_score = rating_score;
	}


	@Override
	public String toString() {
		return "Rating [rating_no=" + rating_no + ", rating_score=" + rating_score + ", m_id=" + m_id + ", movie_no="
				+ movie_no + "]";
	}
	
	

}
